{
    "geode": "5.0.0-beta.3",
    "gd": {
        "win": "2.2081",
        "android": "2.2081",
        "mac": "2.2081",
        "ios": "2.2081"
    },
    "version": "v1.5.1",
    "id": "ninkaz.editor_utils",
    "name": "NinKaz's Editor Utils",
    "developer": "NinKaz",
    "description": "Editor utilities to make my life easier",
    "links": {
        "source": "https://github.com/flurrybun/ninkaz-editor-utils"
    },
    "tags": ["editor", "enhancement", "utility", "interface", "offline"],
    "dependencies": {
        "geode.node-ids": "v1.22.0"
    },
    "resources": {
        "spritesheets": {
            "editor-sheet": [
                "resources/multi-edit/*.png",
                "resources/delete-filter/*.png",
                "resources/advanced-paste-state/*.png",
                "resources/advanced-filter/*.png"
            ]
        }
    },
    "settings": {
        "enable-aps": {
            "type": "bool",
            "name": "Enable Advanced Paste State",
            "description": "Shows a menu to change options when pasting object state. When disabled, the Paste State button works the same as in the vanilla editor.",
            "default": true
        },
        "hide-ui-key": {
            "type": "string",
            "name": "Hide UI Key",
            "description": "The key used to hide the trigger UI. Useful for AZERTY keyboard layouts, which use shift for the number keys.",
            "one-of": ["CTRL/CMD", "ALT", "SHIFT"],
            "default": "SHIFT",
            "platforms": ["desktop"]
        }
    }
}
